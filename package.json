{
  "name": "react-web-store",
  "version": "1.0.0",
  "description": "This is a project to build a web store with React.js for learning purposes.",
  "main": "webpack.config.js",
  "scripts": {
    "start": "npm run build",
    "pug-prod": "pug -o . src/pug/index.pug",
    "sass-prod": "node-sass -o app/css src/sass/main.sass --output-style compressed",
    "build": "webpack -d && webpack-dev-server --content-base src/ --inline --hot",
    "build:prod": "npm run pug-prod && npm run sass-prod && webpack -p"
  },
  "author": "Balázs Orbán",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "jsx-loader": "^0.13.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-router": "^3.0.1",
    "react-swipe": "^5.0.6",
    "swipe-js-iso": "^2.0.3",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.1"
  }
}
